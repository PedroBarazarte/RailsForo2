<% @article.comments.each do |comment| %>
    <p>
      <strong>Commenter:</strong>
      <%= comment.commenter %>
    </p>
   
    <p>
      <strong>Comment:</strong>
      <%= comment.body %>
    </p>
    <%= link_to 'Destroy Comment', article_comments_path(@article,comment),
               method: :delete, data: { confirm: 'Are you sure?' } %>
  <% end %>